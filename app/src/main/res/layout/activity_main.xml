        android:background="#FFFFFF"
        android:textColor="#000000"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/m1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="M"
        android:background="#000000"
        android:textColor="#FFFFFF"
        android:layout_marginBottom="8dp" />

    <Button
        android:id="@+id/n1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
